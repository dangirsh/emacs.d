defaults
tls on
auth on
tls_starttls on
#tls_trust_file /etc/ssl/certs/ca-certificates.crt
tls_certcheck off
logfile ~/.msmtp.log

# Gmail account
account gmail
host smtp.gmail.com
port 587
user pemacsmail1@gmail.com
from pemacsmail1@gmail.com
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.mailpass-gmail.gpg"

# Qsdfgh account
account qsdfgh
host smtp.gmail.com
port 587
user pemacsmail2@gmail.com
from pemacsmail2@gmail.com
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.mailpass-qsdfgh.gpg"

# Set default
account default: gmail
